import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    
    static int arrSum(int i, int j, int[][] arr){
        return   arr[i][j]   +  arr[i][j+1]  +   arr[i][j+2]    //[][][]
                             + arr[i+1][j+1] +                  //  []
                 arr[i+2][j] + arr[i+2][j+1] + arr[i+2][j+2];   //[][][]       
    }
    
    static int solver(int[][] arr){
        int max = -100, sum = 0;
        for(int i = 0; i < 4; i++){
            for(int j = 0; j < 4; j++){
                sum = arrSum(i, j, arr);
                if(sum > max){ 
                    max = sum;
                }
            }
        }
        return max;   
    }
    
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int arr[][] = new int[6][6];
       
        for(int i = 0; i < 6; i++) {
            for(int j = 0; j < 6; j++) {
                arr[i][j] = in.nextInt();
            }
        }
        System.out.println(solver(arr));
    }
}
